{
    "version": "2.0",
    "logging": {
      "applicationInsights": {
        "samplingSettings": {
          "isEnabled": true,
          "excludedTypes": "Request"
        },
        "enableLiveMetricsFilters": true
      },
      "logLevel": {
        "default": "Information", 
        "Host.Results": "Information", 
        "Function": "Information", 
        "Host.Aggregator": "Information",
        "Microsoft": "Warning", 
        "System": "Warning"
      }
  }
}